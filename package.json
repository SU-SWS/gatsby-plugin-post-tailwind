{
  "name": "gatsby-plugin-post-tailwind",
  "version": "1.0.0-dev",
  "description": "Run tailwind post generation of the html files.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SU-SWS/gatsby-plugin-post-tailwind.git"
  },
  "keywords": [
    "gatsby",
    "plugin",
    "post-build",
    "tailwind",
    "purgecss",
    "tailwindcss"
  ],
  "author": "Stanford Web Services",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/SU-SWS/gatsby-plugin-post-tailwind/issues"
  },
  "homepage": "https://github.com/SU-SWS/gatsby-plugin-post-tailwind#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "autoprefixer": "^10.2.5",
    "fs-extra": "^9.1.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.1.0",
    "precss": "^4.0.0",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "babel-preset-gatsby-package": "^1.3.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "^3.0"
  },
  "engines": {
    "node": ">=14.8.0"
  }
}
